	"RemoveFiles_Stage1": [
		"/etc/hosts",
		"/etc/resolv.conf"
	],
	"RemoveDirs_Stage1": [
	],

	"Commands_Stage1_0": [
		"export DEBIAN_FRONTEND=noninteractive",
		"apt-get update",
		"apt-get install -y coreutils pigz"
	],

	"Commands_Stage1": [
		"export ROOTFS=./RootFS",
		"mount -t proc /proc $ROOTFS/proc/",
		"mount --rbind --make-rslave /sys $ROOTFS/sys/",
		"mount --rbind --make-rslave /dev $ROOTFS/dev/",
		"mount --rbind --make-rslave /dev/pts $ROOTFS/dev/pts/"
	],

	"Commands_InChroot": [
		"echo \"nameserver 8.8.8.8\" > /etc/resolv.conf",
		"echo \"US/Eastern\" >/etc/timezone",
		"ln -fs /usr/share/zoneinfo/America/New_York /etc/localtime",
		"chmod 1777 /tmp",
		"export DEBIAN_FRONTEND=noninteractive",
		"apt-get update",
		"apt-get clean",
		"dpkg --add-architecture i386",
		"apt-get update",
		"apt-get dist-upgrade -y"
	],

	"Commands_InChroot2": [
		"apt-get clean"
	],
	"Commands_Stage2": [
		"export ROOTFS=./RootFS",
		"umount $ROOTFS/proc/",
		"umount -R $ROOTFS/sys/",
		"umount -R $ROOTFS/dev/pts/",
		"umount -R $ROOTFS/dev/"
	],

	"RemoveFiles_Stage2": [
		"/etc/hosts",
		"/etc/resolv.conf",
		"/etc/timezone",
		"/etc/localtime",
		"/etc/passwd"
	],

	"RemoveDirs_Stage2": [
		"/boot",
		"/dev",
		"/home",
		"/media",
		"/mnt",
		"/proc",
		"/root",
		"/srv",
		"/tmp",
		"/var/cache/apt",
		"/var/lib/apt",
		"/sys",
		"/opt"
	],

	"Commands_Stage3": [
		"ln -s -f ../../usr/lib/x86_64-linux-gnu/ld-linux-x86-64.so.2 lib64/",
		"rm bin/pidof",
		"ln -s ../sbin/killall5 bin/pidof"
	],

